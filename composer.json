{
  "name": "bytic/document-generator",
  "type": "library",
  "description": "document generator library",
  "keywords": [
    "facebook",
    "pixel"
  ],
  "homepage": "https://github.com/bytic/document-generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\DocumentGenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\DocumentGenerator\\Tests\\": "tests/src",
      "ByTIC\\DocumentGenerator\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^8.1",
    "bytic/orm": "^1.0|^2.0",
    "bytic/data-objects": "^1.0|^2.0",
    "bytic/medialibrary": "^0.9|^1.0",
    "bytic/models-smart-properties": "^1.0|^2.0",
    "symfony/http-foundation": "^5.0|^6.0|^7.0",
    "setasign/fpdf": "^1.8",
    "setasign/fpdi": "^2.3",
    "tecnickcom/tcpdf": "^6.0",
    "phpoffice/phpspreadsheet": "^1.4"
  },
  "require-dev": {
    "bytic/translation": "^1.0|^2.0",
    "bytic/controllers": "^1.0|^2.0",
    "bytic/cache": "^1.0|^2.0",
    "bytic/console": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0"
  },
  "suggest": {
    "bytic/controllers": "^1.0|^2.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "bytic/phpqatools": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
