{
  "name": "bytic/document-generator",
  "type": "library",
  "description": "document generator library",
  "keywords": [
    "facebook",
    "pixel"
  ],
  "homepage": "https://github.com/bytic/document-generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\DocumentGenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\DocumentGenerator\\Tests\\": "tests/src",
      "ByTIC\\DocumentGenerator\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=7.0",
    "bytic/orm": "^0.9|^1.0",
    "bytic/medialibrary": "^0.8|^0.9",
    "bytic/models-smart-properties": "^0.9|^1.0",
    "symfony/http-foundation": "^2.7|^3.0",
    "setasign/fpdi-tcpdf": "^2.0",
    "phpoffice/phpspreadsheet": "^1.4"
  },
  "require-dev": {
    "bytic/translation": "^0.9|^1.0",
    "bytic/controllers": "^0.9|^1.0",
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  },
  "suggest": {
    "bytic/controllers": "^0.9"
  }
}
